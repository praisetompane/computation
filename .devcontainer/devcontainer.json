{
    "name": "computation",
    "build": {
        "dockerfile": "../Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "ms-python.black-formatter",
                "ms-python.debugpy",
                "ms-python.mypy-type-checker",
                "scala-lang.scala",
                "vscjava.vscode-java-pack",
                "scalameta.metals"
            ],
            "settings": {}
        }
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/computation,type=bind,consistency=delegated",
    "workspaceFolder": "/computation"
}