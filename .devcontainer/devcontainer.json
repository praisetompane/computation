{
	"name": "computation_and_information",
	"image": "mcr.microsoft.com/devcontainers/python",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.black-formatter",
				"ms-python.debugpy",
				"ms-python.mypy-type-checker"
			],
			"settings": {}
		}
	},
	"workspaceMount": "source=${localWorkspaceFolder},target=/computation_and_information,type=bind,consistency=delegated",
	"workspaceFolder": "/computation_and_information"
}